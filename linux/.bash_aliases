alias python='python3'
alias pip='pip3'
alias cls='clear'
alias node='nodejs'
alias cp='cp -i'
alias ..='cd ..'
alias ...='cd ../..'
alias e='exit'
alias fuck='sudo !!'
alias temps='watch -t -n 1 "cat /proc/cpuinfo | grep \"cpu MHz\" && sensors && nvidia-settings -q gpucoretemp | grep Attribute"'
alias weather='curl --silent http://wttr.in/ | head -7'
feh='feh --scale-down '
alias tmp='cd /tmp'


# shortcuts 
alias thunderbird='~/thunderbird/thunderbird'
alias dl="cd ~/Downloads"
alias proj="cd ~/Documents/Programming"
alias anime="cd ~/Videos/anime/"


# open emacs in the terminal quickly with seperate init file
alias tmacs='emacs -nw -q -l ~/.tmacs.el'
